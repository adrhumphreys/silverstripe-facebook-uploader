{
    "name": "adrhumphreys/silverstripe-facebook-uploader",
    "description": "Upload images from Facebook",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "facebook/graph-sdk": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Adrhumphreys\\FacebookUploader\\": "src/",
            "Adrhumphreys\\FacebookUploader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "js/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
